body {
		background: #4cd6ff;
		padding: 50px;
		font: 14px "Lucida Grande", Helvetica, Arial, sans-serif;
}

#download {
	margin: 0 auto;
	text-align: center;
	width: 120px;
	height: 45px;
	line-height: 45px;
	vertical-align: middle;
	padding: 5px;
	text-shadow: 0 1px 1px grey;
	background-color: white;
	border-radius: 8px;
}

a {
	text-align: center;
	
	color: #00122d;
}

.boat {
	margin-top: 20vh;
	height: 500px;
	width: 500px;
	margin: auto;
}

td {
	border: 1px solid #7faef9;
	padding: 5px;
	border-radius: 4px
}
